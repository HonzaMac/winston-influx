    "node": ">=12"
  },
  "homepage": "https://github.com/honzamac/winston-influx#readme",
  "dependencies": {
    "influx": "^5.7.0",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^4.4.1",
    "@stryker-mutator/jest-runner": "^4.4.1",
    "@types/jest": "^26.0.20",
